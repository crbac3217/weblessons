<%- include("./partials/header"); -%>
<h1>Home</h1>
<p> <%= homeContent %></p>
<hr></hr>
<% posts.forEach(function(post){%>
    <div class="post">
    <h1><%= post.postTitle %></h1> 
    <p><%= post.postBody.slice(0,100) + " ..." %> <a href="posts/<%= post.postTitle %>"> Read More</a></p>
    <hr></hr>
    </div> 
<%}) %> 
<%- include("./partials/footer"); -%>